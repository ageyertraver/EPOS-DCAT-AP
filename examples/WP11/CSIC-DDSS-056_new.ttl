<SusceptibilityMap> a epos:WebService;
	schema:identifier " SusceptibilityMap";
	schema:description "Spatial probability analysis/maps – Susceptibility maps";
	schema:name "Spatial probability analysis/maps – Susceptibility maps";
	schema:datePublished "2018-09-01T00:00:00Z"^^xsd:dateTime;
	schema:dateModified "2018-09-01T12:00:00Z"^^xsd:dateTime;
	schema:provider <http://isni.org/isni/0000000122036192>;
	schema:keywords "Spatial probability analysis", "Spatial probability maps", "volcanology", "hazard assessment", "volcanic hazard", "Susceptibility maps";
	hydra:entrypoint " https://volcanboxws.obsea.es/"^^xsd:anyURI; 
        # geometry of zone: Canary Islands
        locn:geometry "POLYGON((-18.4 27.4, -18.4 29.6, -13 29.6, -13 27.4))"^^gsp:wktLiteral;
        # geometry of zone: Lanzarote
        locn:geometry "POLYGON((-14 29.4, -13 29.4, -13 28.75, -14 28.75))"^^gsp:wktLiteral;
       # geometry of zone: El Hierro
        locn:geometry "POLYGON((-18.24 27.88, -17.78 27.88, -17.78 27.6, -18.24 27.6))"^^gsp:wktLiteral;
             # geometry of zone: La Garrotxa
        locn:geometry "POLYGON((2.4 42.27, 2.8 42.27, 2.8 42, 2.4 42))"^^gsp:wktLiteral;
             # geometry of zone: Deception Island
        locn:geometry "POLYGON((-60.8 -62.8, -60.48 -62.8, -60.48 -63.03, -60.8 -63.03))"^^gsp:wktLiteral;
             # geometry of zone: San Miguel
        locn:geometry "POLYGON((-88.56 13.74, -87.89 13.74, -87.89 13.11, -88.56 13.11))"^^gsp:wktLiteral;
    ]; 
	hydra:supportedOperation <SusceptibilityMap/operation>;
	dcat:theme <epos:SusceptibilityMap>;
	dcat:contactPoint <http://orcid.org/0000-0002-8803-6504/contactPoint>;
	
.

#########
# Operation
#########

<SusceptibilityMap/operation> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
	hydra:returns "application/json";
	hydra:property[ a hydra:IriTemplate;
		hydra:template "https://volcanboxws.obsea.es:5000/maptype{?type,volcan}"^^xsd:string;
			
			hydra:mapping[ a hydra:IriTemplateMapping;
          			hydra:variable "type"^^xsd:string;
				rdfs:range "xsd:string";
				rdfs:label "Type";
				hydra:required "true"^^xsd:boolean;
				http:paramValue "susceptibility" ;
				schema:defaultValue "susceptibility" ;				
        		];
		
			hydra:mapping[ a hydra:IriTemplateMapping;
				hydra:variable "volcan"^^xsd:string;
				rdfs:range "xsd:string";
				rdfs:label "Volcano name that exposes hazard map";				
				http:paramValue "DeceptionIsland";
				http:paramValue "ElHierro";
				http:paramValue "LaGarrotxa";
				http:paramValue "SanMiguel";
				http:paramValue "Lanzarote"; 
				schema:defaultValue "DeceptionIsland";
				hydra:required "false"^^xsd:boolean;
			];
	];
. 

