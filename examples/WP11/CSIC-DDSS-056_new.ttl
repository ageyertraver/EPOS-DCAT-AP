#########
# Dataset
######### 

<WP11/SusceptibilityMap> a dcat:Dataset;
        dct:title "Spatial probability analysis/maps of volcanic activity";
        dct:identifier "WP11/Susceptibility map";
        adms:identifier [ a adms:Identifier;
         	adms:schemaAgency "DDSS-ID";
        	skos:notation "WP11-DDSS-056";
        ];
        dct:description "Spatial probability analysis/maps of the probability of occurrence of a specific area to host a future eruptive event. These maps are static and based on the geological record including information about fractures, past eruptions, etc.";
        ## example of frequency using a controlled vocabulary
	dct:type "http://purl.org/dc/dcmitype/Collection"^^xsd:anyURI;
        dct:accrualPeriodicity "http://purl.org/cld/freq/irregular"^^xsd:anyURI;
        dct:created "2018-09-01"^^xsd:date;
	dct:spatial [ a dct:Location;
        # geometry of zone: Canary Islands
        locn:geometry "POLYGON((-18.4 27.4, -18.4 29.6, -13 29.6, -13 27.4))"^^gsp:wktLiteral;
        # geometry of zone: Lanzarote
        locn:geometry "POLYGON((-14 29.4, -13 29.4, -13 28.75, -14 28.75))"^^gsp:wktLiteral;
       # geometry of zone: El Hierro
        locn:geometry "POLYGON((-18.24 27.88, -17.78 27.88, -17.78 27.6, -18.24 27.6))"^^gsp:wktLiteral;
             # geometry of zone: La Garrotxa
        locn:geometry "POLYGON((2.4 42.27, 2.8 42.27, 2.8 42, 2.4 42))"^^gsp:wktLiteral;
             # geometry of zone: Deception Island
        locn:geometry "POLYGON((-60.8 -62.8, -60.48 -62.8, -60.48 -63.03, -60.8 -63.03))"^^gsp:wktLiteral;
             # geometry of zone: San Miguel
        locn:geometry "POLYGON((-88.56 13.74, -87.89 13.74, -87.89 13.11, -88.56 13.11))"^^gsp:wktLiteral;
    ];      

	dcat:theme <epos:SusceptibilityMap>;
        dcat:keyword "Spatial probability analysis", "Spatial probability maps", "volcanology", "hazard assessment", "volcanic hazard", "Garrotxa Volcanic Field", "Lanzarote", "El Hierro", "geohazards", "Deception Island", "San Miguel";
        dcat:contactPoint <http://orcid.org/0000-0002-8803-6504/contactPoint>;
	dct:publisher <http://isni.org/isni/0000000122036192>;
        dcat:distribution <WP11/SusceptibilityMap/WSDistribution>;
        
.

